{
  "id": "basic",
  "layers": [
    {
      "id": "background",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "background-color": "rgba(243, 245, 247, 1)"
      },
      "type": "background"
    },
    {
      "filter": ["all", ["!=", "intermittent", 1], ["!=", "brunnel", "tunnel"]],
      "id": "water",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": "rgba(153, 213, 255, 1)"
      },
      "source": "openmaptiles",
      "source-layer": "water",
      "type": "fill"
    },
    {
      "filter": [
        "all",
        [
          ">=",
          "admin:level",
          3
        ],
        [
          "<=",
          "admin:level",
          8
        ],
        [
          "!=",
          "maritime",
          1
        ]
      ],
      "id": "boundary-land-level-4",
      "layout": {
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#ABBAC4",
        "line-dasharray": [
          3,
          1,
          1,
          1
        ],
        "line-width": {
          "base": 1.4,
          "stops": [
            [
              4,
              0.4
            ],
            [
              5,
              1
            ],
            [
              12,
              3
            ]
          ]
        }
      },
      "source": "openmaptiles",
      "source-layer": "boundary",
      "type": "line"
    },
    {
      "filter": [
        "all",
        [
          "!=",
          "maritime",
          1
        ],
        [
          "==",
          "disputed",
          1
        ]
      ],
      "id": "boundary-land-disputed",
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(147, 167, 180, 1)",
        "line-dasharray": [
          1,
          3
        ],
        "line-width": {
          "base": 1,
          "stops": [
            [
              0,
              0.6
            ],
            [
              4,
              1.4
            ],
            [
              5,
              2
            ],
            [
              12,
              8
            ]
          ]
        }
      },
      "source": "openmaptiles",
      "source-layer": "boundary",
      "type": "line"
    },
    {
      "filter": [
        "all",
        [
          "in",
          "admin:level",
          2,
          4
        ],
        [
          "==",
          "maritime",
          1
        ]
      ],
      "id": "boundary-water",
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "none"
      },
      "minzoom": 4,
      "paint": {
        "line-color": "rgba(154, 189, 214, 1)",
        "line-opacity": {
          "stops": [
            [
              6,
              0.6
            ],
            [
              10,
              1
            ]
          ]
        },
        "line-width": {
          "base": 1,
          "stops": [
            [
              0,
              0.6
            ],
            [
              4,
              1.4
            ],
            [
              5,
              2
            ],
            [
              12,
              8
            ]
          ]
        }
      },
      "source": "openmaptiles",
      "source-layer": "boundary",
      "type": "line"
    },
    {
      "id": "highway",
      "filter": ["==", "$type", "LineString"],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(147, 167, 180, 1)",
        "line-width": 2.5
      },
      "source": "openmaptiles",
      "source-layer": "transportation",
      "type": "line"
    },
    {
      "id": "building-top",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": "rgba(253, 231, 204, 1)",
        "fill-opacity": {
          "base": 1,
          "stops": [
            [13, 0],
            [16, 1]
          ]
        },
        "fill-outline-color": "rgba(249, 183, 102, 1)",
        "fill-translate": {
          "base": 1,
          "stops": [
            [14, [0, 0]],
            [16, [-2, -2]]
          ]
        }
      },
      "source": "openmaptiles",
      "source-layer": "building",
      "type": "fill"
    },
    {
      "id": "place",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "filter": ["==", "$type", "Point"],
      "layout": {
        "text-field": "{name:latin}\n{name:nonlatin}",
        "text-font": ["Noto Sans Bold"],
        "text-letter-spacing": 0.1,
        "text-max-width": 9,
        "text-size": {"base": 1.2, "stops": [[12, 10], [15, 14]]},
        "text-transform": "uppercase",
        "visibility": "visible"
      },
      "paint": {
        "text-color": "#633",
        "text-halo-color": "rgba(255,255,255,0.8)",
        "text-halo-width": 1.2
      }
    }
  ],
  "name": "basic",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "tiles": ["http://localhost:8081/omt/{z}/{x}/{y}.mlt"],
      "maxzoom": 14
    }
  },
  "glyphs": "http://localhost:8081/omt/fonts/{fontstack}/{range}.pbf",
  "version": 8
}